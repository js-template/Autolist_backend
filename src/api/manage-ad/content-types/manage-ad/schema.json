{
  "kind": "collectionType",
  "collectionName": "manage_ads",
  "info": {
    "singularName": "manage-ad",
    "pluralName": "manage-ads",
    "displayName": "Manage Ad",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Condition": {
      "type": "enumeration",
      "enum": [
        "Used",
        "New"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Price": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ads-category.ads-category"
    },
    "Location": {
      "type": "customField",
      "customField": "plugin::google-maps.location-picker"
    },
    "Negotiable": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "Title"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
